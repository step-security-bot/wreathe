{
  "name": "@wreathe-js/core",
  "version": "0.1.0-alpha.1",
  "description": "Connects the engine to the transmission.",
  "homepage": "https://wreathe.dev",
  "bugs": {
    "url": "https://github.com/wreathe-js/wreathe/issues",
    "email": "contact@wreathe.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wreathe-js/wreathe.git",
    "directory": "packages/core"
  },
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "require": "./dist/server.cjs",
      "import": "./dist/server.mjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "server.d.ts",
    "server.js"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "clean": "rm -rf dist",
    "fmt:check": "prettier --check \"**/*.{ts,tsx,json,md}\"",
    "fmt": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "axios": "^1.2.1",
    "deepmerge": "^4.2.2",
    "nprogress": "^0.2.0",
    "qs": "^6.11.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.17",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7"
  },
  "publishConfig": {
    "access": "public"
  }
}
